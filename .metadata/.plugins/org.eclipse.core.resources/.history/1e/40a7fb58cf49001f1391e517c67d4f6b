import java.util.Scanner;

public class A7_Functions {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int a, b, c;
			
		
		System.out.printf("Intome o primeiro inteiro: ");
		a = sc.nextInt();				
		System.out.printf("Intome o segundo inteiro: ");
		b= sc.nextInt();
		System.out.printf("Intome o terceiro inteiro: ");
		c = sc.nextInt();
		
		
		if(a > b && a > c) {
			System.out.println("Maior numero é o "+a);
			
		}else if(b > c) {
			System.out.println("Maior numero é o "+b);
			
		}else {
			System.out.println("Maior numero é o "+c);
		}
		
		
		
		sc.close();
	}

}
